<h1>New Wiki</h1>

 <div class="row">

   <div class="col-md-8">
     <%= render partial: 'form', locals: { wiki: @wiki } %>
   </div>

   <% if @wiki.private %>
    <div class="form-group">
      <%= render partial: 'collaborators/form', locals: { wiki: @wiki, user: current_user} %>
    </div>
  <% end %>
 </div>
